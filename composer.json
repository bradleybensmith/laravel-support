{
    "name": "bram1028/laravel-support",
    "description": "Adds common functionality to Laravel.",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bradley B. Smith",
            "email": "bram1028@icloud.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "hanneskod/classtools": "^1.2.1",
        "symfony/finder": "^4|^5"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Support\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Support\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
